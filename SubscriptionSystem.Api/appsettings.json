{
  "ConnectionStrings": {
    "DefaultConnection": "<Database Connection String>",
    "AzureServiceBus": "<Azure Service Bus Connection String>"
  },
  "ServiceBus": {
    "TopicName": "events-topic"
  },
  "Logging": {
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore": "Error"
    },
    "Console": {
      "IncludeScopes": false
    },
    "AzureAppServicesFile": {
      "IncludeScopes": false,
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore": "Error"
      }
    }
  },
  "AllowedHosts": "*"
}
